{
    "name": "Company Crawler: Find company reviews",
    "version": "1.0.1",
    "description": "This extension allows you to easily search for a company's reviews, saving you time and hassle while searching for a job!",
    "manifest_version": 3,
    "author": "Kody",
    "action":{
        "default_title": "Company Crawler: Find company reviews",
        "default_popup": "popup.html"
    },
    "permissions": [
        "contextMenus"
      ],
    "icons": {
      "16": "logo-16.png",
      "48": "logo-48.png",
      "128": "logo-128.png"
      },
    "background": {
      "service_worker": "script.js"
    }
}